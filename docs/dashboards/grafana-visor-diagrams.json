{
  "__inputs": [],
  "title": "Visor Diagrams",
  "timezone": "browser",
  "panels": [
    {
      "type": "bargauge",
      "title": "Diagram Blocks by Origin (5m rate)",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "PROM_DS" },
          "expr": "sum(rate(visor_diagram_blocks_total[5m])) by (origin)"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    }
  ],
  "version": 1,
  "editable": true
}
