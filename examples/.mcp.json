{
  "mcpServers": {
    "code_analyzer": {
      "command": "python3",
      "args": ["./mcp/analyzer.py"],
      "env": {
        "ANALYSIS_LEVEL": "comprehensive"
      }
    },
    "security_scanner": {
      "command": "node",
      "args": ["./mcp/security.js"],
      "env": {
        "SCAN_DEPTH": "full",
        "INCLUDE_DEPENDENCIES": "true"
      }
    },
    "performance_profiler": {
      "command": "node",
      "args": ["./tools/performance-analyzer.js"],
      "env": {
        "PROFILE_MODE": "detailed",
        "OUTPUT_FORMAT": "json"
      }
    },
    "dependency_checker": {
      "command": "npx",
      "args": ["npm-check", "--json"],
      "env": {
        "CHECK_UNUSED": "true"
      }
    }
  }
}